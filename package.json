{
  "name": "ai-teacher-assistant",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered teacher assistant that automates grading and provides personalized feedback",
  "scripts": {
    "start": "react-app-rewired start",
    "start:fast": "set REACT_APP_USE_MOCK_SERVICES=true & set GENERATE_SOURCEMAP=false & react-app-rewired start",
    "start:prod": "set REACT_APP_USE_MOCK_SERVICES=false & react-app-rewired start",
    "build": "set \"CI=\" && react-app-rewired build",
    "build:netlify": "react-app-rewired build",
    "analyze": "set ANALYZE=true && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "security-audit": "node scripts/securityAudit.js",
    "production-optimize": "node scripts/productionOptimizer.js",
    "performance-test": "node scripts/performanceAnalysis.js",
    "test:comprehensive": "npm test -- --coverage --watchAll=false",
    "audit:security": "npm run security-audit && npm audit",
    "optimize:production": "npm run production-optimize && npm run build",
    "monitor:bundle": "npm run analyze"
  },
  "dependencies": {
